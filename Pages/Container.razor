@using LayoutDesigner.Interfaces
@using LayoutDesigner.Models
<div class="container">
	<CascadingValue Value="this">
		@ChildContent
	</CascadingValue>
</div>

@code {
	[Parameter]
	public RenderFragment? ChildContent { get; set; }

	public Type Payload { get; set; }
	public DragInformation PayloadComponent { get; set; }
}
